#include <iostream>
using namespace std;

int main() {
	double a, b, c;
	cin>>a;
	cin>>b;
	cin>>c;
	if ((a < b) && (a < c))  {
		if ( b < c) cout<<a<<" "<<b<<" "<<c;
		else cout<<a<<" "<<c<<" "<<b;
	}
	else if ((a < b) && (a > c)) cout<<c<<" "<<a<<" "<<b;
	else if ((a > b) && (a < c)) cout<<b<<" "<<a<<" "<<c;
	else {
		if (b < c) cout<<b<<" "<<c<<" "<<a;
		else cout<<c<<" "<<b<<" "<<a;
	}
	return 0;
