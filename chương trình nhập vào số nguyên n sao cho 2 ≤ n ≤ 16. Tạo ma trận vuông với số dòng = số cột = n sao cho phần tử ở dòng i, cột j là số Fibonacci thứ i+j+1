define
include
class
###End banned keyword*/

#include <iostream>
using namespace std;
#define MAX 16


void Nhapmang(int a[][MAX], int &n){
	cin>>n;
}

int fibonacci(int n) {
    if (n <= 0) return 0;
    if (n == 1) return 1;
    return fibonacci(n - 1) + fibonacci(n - 2);
}

void Xuatmang(int a[][MAX], int n){
for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            int fib = fibonacci(i + j + 1);
            cout << fib << " ";
        }
        cout << endl;
    }
}
int main()
{
    int n, a[MAX][MAX];
	Nhapmang(a,n);
	Xuatmang(a,n);
    return 0;
}
