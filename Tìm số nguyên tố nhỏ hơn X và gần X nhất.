#include <iostream>
#include <cmath>

using namespace std;

bool isPrime(int number) {
    if (number <= 1) {
        return false;
    }
    for (int i = 2; i <= sqrt(number); i++) {
        if (number % i == 0) {
            return false;
        }
    }
    return true;
}

int kiemtrasonguyento (int n) {
	int gcd = 1;
	for (int i = 2; i < n; i++) {
		if (isPrime(i)) gcd = i;
	}
	return gcd;
}

int main () {
	int n;
	cin>>n;
	cout<<kiemtrasonguyento(n);
	return 0;
	
}
