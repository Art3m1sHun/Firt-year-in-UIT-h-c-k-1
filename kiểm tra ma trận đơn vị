#include <iostream>
#define MAXR 100
#define MAXC  100
using namespace std;

void NhapMaTran(int a[MAXR][MAXC], int &dong) {
    cin >> dong;
    for (int i = 0; i < dong; i++) {
        for (int j = 0; j < dong; j++)
            cin >> a[i][j];
    }
}

bool isMaTranDonVi(int matrix[MAXR][MAXC], int n){
bool isIdentity = true;
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            if (i == j && matrix[i][j] != 1) {
                isIdentity = false;
                break;
            }
            if (i != j && matrix[i][j] != 0) {
                isIdentity = false;
                break;
            }
        }
        if (!isIdentity) {
            break;
        }
    }

    if (isIdentity) {
        return true;
    } else {
        return false;
    }
}

int main() {
	int a[MAXR][MAXC], n;
	NhapMaTran(a,n);
	std::cout << std::boolalpha<<isMaTranDonVi(a,n);
	return 0;
}
