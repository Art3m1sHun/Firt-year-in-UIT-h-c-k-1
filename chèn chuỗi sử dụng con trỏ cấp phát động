#include <iostream>
#include <string.h>
#define MAX 100
using namespace std;

char* chenchuoi(char* s, char* s1, int k) {
    int len_s = strlen(s);
    int len_s1 = strlen(s1);
    char* result = new char[len_s + len_s1 + 1];
    if(k < 0 || k > (len_s))return 0; 
    else{
    for (int i = 0; i < k; i++) {
        result[i] = s[i]; // chèn chuối từ vị trí 0 đến vị trí k
    }

    for (int j = 0; j < len_s1; j++) {
        result[k + j] = s1[j]; // chèn chuối mới vào bắt đầu từ vị trí k
    }

    for (int i = k; i < len_s; i++) {
        result[len_s1 + i] = s[i];// chèn phần còn lại của chuỗi
    }

    result[len_s + len_s1] = '\0'; // kết thúc chuỗi

    return result;
}
}

int main() {
	int k;
	char *s = NULL, *s1 = NULL;
	s = new char[MAX];
	s1 = new char[MAX];
	gets(s);
	gets(s1);
	cin >> k;
	if(chenchuoi(s,s1,k) == 0) cout<<"Vi tri "<<k<<" khong thoa dieu kien.";
	else {
	char *result = chenchuoi(s,s1,k);
	cout<<result;
	delete[] result;
	delete[] s;
	delete[] s1;
}
	return 0;
}
