#include <iostream>
#define MAX 100
using namespace std;

void input(float arr[MAX][MAX], int &dong, int &cot) {
    cin >> dong >> cot;
    for (int i = 0; i < dong; i++) {
        for (int j = 0; j < cot; j++) {
            cin >> arr[i][j];
        }
    }
}

bool checkFrobenius(float arr[MAX][MAX], int dong, int cot){
    if (dong != cot) return false;
    for (int i = 0; i < dong; i++){ //co toi da mot cot ma cac phan tu duoi duong cheo chinh co the nhan gia tri bat ki
        int count = 1;
        for (int j = 0; j < i; j++){
            if (arr[i][j] != 0)
                count++;
        }
        if (arr[i][i] != 1 || count > 2) return false;//tat ca cac phan tu tren cheo chinh bang 1
    }
    for (int j = 0; j < dong; j++){ //cac phan tu khac bang 0
        for (int i = 0; i < j; i++){
            if (arr[i][j] != 0) return false;
        }
    }
    return true;
}

int main() {
    float arr[MAX][MAX];
    int dong, cot;
    input(arr, dong, cot);
    if (checkFrobenius(arr, dong, cot)) {
        cout << "Yes" << endl;
    } else {
        cout << "No" << endl;
    }

    return 0;
}
