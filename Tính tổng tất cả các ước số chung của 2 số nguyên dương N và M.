
#include <iostream>


void input(int &a) {
	std::cin>>a;
}

int input() {
	int a;
	std::cin>>a;
	return a;
}

int TongUocChung(int a, int b) {
	int smaller = 0;
	if (a < b) smaller = a;
	else smaller = b;
	int sum = 0;
	for (int i = 1; i <= smaller; i++) {
		if ((a % i == 0) && (b % i == 0)) sum +=i;
	}
	return sum;
}

int main(){
    int a, b;
    a=input();
    input(b);
    std::cout << TongUocChung(a, b);
    return 0;
}
