#include <iostream>
#define MAX 100
using namespace std;

void nhapmang(float **s, int m, int n) {
    for (int i = 0; i < m; i++) {
        for (int j = 0; j < n; j++) {
            cin >> s[i][j];
        }
    }
}

void xuatmang(float **s, int m, int n) {
    for (int i = 0; i < m; i++) {
        for (int j = 0; j < n; j++) {
            cout << s[i][j] << " ";
        }
        cout << endl;
    }
}

int main() {
    int m, n;
    cin >> m >> n;

    // Dynamic memory allocation for a two-dimensional array
    float **s = new float *[m];
    for (int i = 0; i < m; i++) {
        s[i] = new float[n];
    }

    nhapmang(s, m, n);
    xuatmang(s, m, n);

    // Deallocate memory
    for (int i = 0; i < m; i++) {
        delete[] s[i];
    }
    delete[] s;

    return 0;
}
